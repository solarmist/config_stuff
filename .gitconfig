[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad |[%an] %s%d\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
[branch]
    autosetupmerge = true
	autosetuprebase = always
[color]
    diff = auto
    grep = auto
	ui = true
[core]
	editor = emacs
	excludesfile = /Users/jolson/.gitignore_global
	trustctime = false
[diff]
    color = auto
    mnemonicprefix = true
[grep]
    lineNumber = true
[help]
    format = info
[merge]
    tool = emerge
[user]
	name = Joshua Olson
	email = jolson@machinezone.com
[push]
	default = upstream
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
